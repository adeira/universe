[ignore]
<PROJECT_ROOT>/\.git/.*

[include]

[libs]
./flow-typed/

[lints]
all=error
ambiguous-object-type=off
sketchy-null-bool=off
unclear-type=off
untyped-import=off

[options]
types_first=true
module.ignore_non_literal_requires=true

[strict]
ambiguous-object-type
sketchy-null-bool
unclear-type
untyped-import

[declarations]
.+/node_modules/.+
