{
  "name": "kochka.com.mx",
  "version": "0.0.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "yarn jest --config=.jest-eslint.config.js",
    "test": "yarn jest",
    "fbt:manifest": "fbt-manifest --src=src --src=pages",
    "fbt:collect": "NODE_ENV=development fbt-collect --options=__self --pretty --manifest < .src_manifest.json > .source_strings.json",
    "fbt:translate": "fbt-translate --translations translations/*.json --jenkins > translatedFbts.json",
    "fbt": "yarn run fbt:manifest && yarn run fbt:collect && yarn run fbt:translate"
  },
  "dependencies": {
    "@adeira/sx": "^0.11.0",
    "@babel/register": "^7.11.5",
    "babel-plugin-fbt": "^0.15.1",
    "babel-plugin-fbt-runtime": "^0.9.12",
    "fbt": "^0.15.0",
    "next": "^9.5.3",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "@adeira/babel-preset-adeira": "^1.0.0",
    "@adeira/eslint-config": "^3.1.1",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.9.0",
    "flow-bin": "^0.134.0",
    "jest": "^26.4.2",
    "prettier": "^2.1.2",
    "react-axe": "^3.5.3"
  }
}
